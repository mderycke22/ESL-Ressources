CREATE TABLE feeds (_id integer PRIMARY KEY, active text, address text, favicon_colour text, favicon_url text, ps integer, ng integer, nt integer, favicon text, favicon_fade text, favicon_border text, link text, subscribers text, feed_name text, updated_seconds);
CREATE TABLE social_feeds (_id integer PRIMARY KEY, ps integer, ng integer, nt integer, social_feed_icon text, social_feed_title text, social_feed_name text);
CREATE TABLE folders (_id integer PRIMARY KEY AUTOINCREMENT, folder_name text UNIQUE );
CREATE TABLE user_table (photo_url text, _id integer PRIMARY KEY, username text);
CREATE TABLE stories (authors text, content text, date text, sharedDate integer, shortDate text, longDate text, feed_id integer, _id text PRIMARY KEY, intelligence_authors integer, intelligence_feed integer, intelligence_tags integer, intelligence_title integer, comment_count integer, share_count integer, socialUserId text, sourceUserId text, shared_user_ids text, public_user_ids text, comment_user_ids text, tags text, permalink text, read text, title text);
CREATE TABLE comments (comment_date text, comment_shareddate text, comment_source_user text, _id text PRIMARY KEY, comment_liking_users text, comment_byfriend text, comment_storyid text, comment_text text, comment_userid text);
CREATE TABLE comment_replies (reply_date text, reply_shortdate text, _id text PRIMARY KEY, comment_id text, reply_text text, reply_userid text);
CREATE TABLE classifiers (_id text, key text, type text, value text);
CREATE TABLE feed_folder_map (feed_folder_name text NOT NULL, feed_feed_id integer NOT NULL, PRIMARY KEY (feed_folder_name, feed_feed_id) );
CREATE TABLE socialfeed_story_map (socialfeed_story_storyid text NOT NULL, socialfeed_story_user_id integer NOT NULL, PRIMARY KEY (socialfeed_story_storyid, socialfeed_story_user_id) );
CREATE TABLE offline_updates (_id integer PRIMARY KEY, update_type integer, update_argument text);
DROP TABLE IF EXISTS feeds;
DROP TABLE IF EXISTS social_feeds;
DROP TABLE IF EXISTS folders;
DROP TABLE IF EXISTS stories;
DROP TABLE IF EXISTS user_table;
DROP TABLE IF EXISTS comments;
DROP TABLE IF EXISTS comment_replies;
DROP TABLE IF EXISTS classifiers;
DROP TABLE IF EXISTS feed_folder_map;
DROP TABLE IF EXISTS socialfeed_story_map;
DROP TABLE IF EXISTS offline_updates;
